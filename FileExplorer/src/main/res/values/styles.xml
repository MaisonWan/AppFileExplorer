<resources>

    <!-- Base application theme. -->
    <style name="FEAppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/fe_colorPrimary</item>
        <item name="colorPrimaryDark">@color/fe_colorPrimaryDark</item>
        <item name="colorAccent">@color/fe_colorAccent</item>
    </style>

    <style name="FEAppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="FEAppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="FEAppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
